{
  "name": "tiller-automation",
  "version": "1.0.0",
  "description": "This connects to a Google Sheet based on the Tiller Foundation Template to read and update rows of the balance history",
  "main": "index.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "prestart": "npm run build",
    "start": "node ./out/index.js",
    "debug": "ts-node ./index.ts",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JetUni/tiller-automation.git"
  },
  "keywords": [
    "tiller",
    "google",
    "sheets",
    "balance",
    "history"
  ],
  "author": "Jarrett Sorensen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JetUni/tiller-automation/issues"
  },
  "homepage": "https://github.com/JetUni/tiller-automation#readme",
  "prettier": {
    "printWidth": 120
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "google-auth-library": "^9.4.1",
    "googleapis": "^129.0.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.7",
    "@types/node": "^20.10.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
